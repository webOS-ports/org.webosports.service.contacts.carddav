{
	"id": "org.webosports.service.cdav",
	"description": "CardDAV Contact Service",
	"engine": "node",
	"activityTimeout": 30,
	"commandTimeout": 300,
	"services": [
		{
			"name": "org.webosports.service.cdav",
			"description": "CardDAV Contact",
			"globalized": false,
			"assistant": "ServiceAssistant",
			"commands": [
				{
					"name": "checkCredentials",
					"assistant": "checkCredentialsAssistant",
					"public": true
				},
				{
					"name": "onCredentialsChanged",
					"assistant": "OnCredentialsChanged",
					"public": true
				},
				{
					"name": "onContactsCreate",
					"assistant": "OnCreate",
					"public": true
				},
				{
					"name": "onCalendarCreate",
					"assistant": "OnCreate",
					"public": true
				},
				{
					"name": "onContactsEnabled",
					"assistant": "OnEnabled",
					"public": true
				},
				{
					"name": "onCalendarEnabled",
					"assistant": "OnEnabled",
					"public": true
				},
				{
					"name": "onCalendarDelete",
					"assistant": "OnDelete",
					"public": true
				},
				{
					"name": "onContactsDelete",
					"assistant": "OnDelete",
					"public": true
				},
				{
					"name": "sync",
					"assistant": "SyncAssistant",
					"public": true
				},
				{
					"name": "discovery",
					"assistant": "DiscoveryAssistant",
					"public": true
				},
				{
					"name": "triggerSlowSync",
					"assistant": "TriggerSlowSyncAssistant",
					"public": true
				},
				{
					"name": "checkStatus",
					"subscribe": true,
					"assistant": "statusAssistant",
					"public": true
				},
				{
					"name": "addItem",
					"assistant": "AddItemAssistant",
					"public": true
				},
				{
					"name": "resources",
					"assistant": "Resources",
					"public": false
				}
			]
		}
	]
}
